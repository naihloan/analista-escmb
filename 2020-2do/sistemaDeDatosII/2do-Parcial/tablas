tablas
create table Marcas (
CodigoMarca int primary key identity (1,1),
Descripcion_Marca varchar(50),
Fecha_Inicio date, 
Estado int,
Fecha_Baja date, 
Observaciones text
)

create table Modelos (
Codigo_Modelo int primary key identity (1,1),
Descripcion_Modelo varchar(50),
Codigo_Marca int,
Fecha_Inicio date,
Estado int, 
Fecha_Baja date,
Observaciones text
)

create table Cargos (
Codigo_Cargo int primary key identity (1,1),
Descripcion_Cargo varchar(50),
Fecha_Inicio date,
Estado int,
Fecha_Baja date,
Observaciones text
)

create table Sucursal (
Codigo_Sucursal int primary key identity (1,1),
Descripcion_Sucursal varchar(50),
Fecha_Inicio date,
Estado int,
Fecha_Baja date,
Responsable text,
Sucursal int,
Direccion varchar(50),
Telefono int,
Mail nvarchar,
Fax nvarchar,
Observaciones text
)


create table Empleados (
Codigo_Empleado int primary key identity (1,1),
